library(Matrix)

p=10;
n=4

G=rsparsematrix(p, p, nnz = 2*p)
Data=matrix( rnorm(p*p,mean=0,sd=1), p, n) 

G=G%*%t(G)+Diagonal(p)
G=abs(G)

W0=Diagonal(p);
X0=Diagonal(p);

W0=W0%*%t(W0)
X0=X0%*%t(X0)

lambda=.0001
max_iter=10
drop_tol=1e-6
term_tol=1e-6

library('BigQuic')
BigQuic (X = Data, inputFileName = NULL, outputFileName = NULL, lambda = 0.5, numthreads = 4, maxit = 5, epsilon = 1e-3, k = 0, memory_size = 8000,verbose = 0, isnormalized = 1, seed = NULL, use_ram = FALSE)


export KMP_DUPLICATE_LIB_OK=TRUE
export R_LD_LIBRARY_PATH=/Users/aryan_mp